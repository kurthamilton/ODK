{
  "AppSettings": {
    "Cors": {
      "AllowedMethods": "GET,POST,PUT,PATCH,DELETE,OPTIONS",
      "AllowedHeaders": "*"
    },
    "Members": {
      "MaxImageSize": 250 /* maximum height or width */
    },
    "Payments": {
      "PayPalApiBaseUrl": "https://api-m.sandbox.paypal.com"
    },
    "Urls": {
      "ActivateAccount": "/{chapter.name}/Account/Activate?token={token}",
      "ConfirmEmailAddressUpdate": "/{chapter.name}/Account/Email/Change/Confirm?token={token}",
      "Event": "/{chapter.name}/Events/{event.id}",
      "EventRsvp": "/{chapter.name}/Events/{event.id}?rsvp=yes",
      "Events": "/{chapter.name}/Events",
      "Media": "/media/{chapter.name}/{name}",
      "PasswordReset": "/{chapter.name}/Account/Password/Reset?token={token}",
      "Unsubscribe": "/{chapter.name}/Account/Emails"
    }
  }
}