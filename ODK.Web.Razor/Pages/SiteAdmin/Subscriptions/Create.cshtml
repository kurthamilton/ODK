@page "/siteadmin/subscriptions/new"
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@using ODK.Web.Common.Components
@using ODK.Web.Common.Routes
@using ODK.Web.Razor.Models.Components
@model CreateModel
@{
    Layout = "_SiteAdminLayout";
    Model.Title = "Create subscription | SiteAdmin";
}

@await Html.PartialAsync("Components/_Breadcrumbs", new BreadcrumbsViewModel
{
    Breadcrumbs = new[]
    {
        new MenuItem { Link = OdkRoutes.SiteAdmin.Subscriptions, Text = "Subscriptions" },
        new MenuItem { Text = "New" }
    }
})

@await Html.PartialAsync("SiteAdmin/_SubscriptionCreateContent")