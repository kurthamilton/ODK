@page "/siteadmin/topics"
@using ODK.Services.Topics
@model TopicsModel
@inject ITopicAdminService TopicAdminService
@{
    Layout = "_SiteAdminLayout";
    Model.Title = "Topics | SiteAdmin";

    var viewModel = await TopicAdminService.GetTopicsAdminPageViewModel(Model.CurrentMemberId);
}

@await Html.PartialAsync("SiteAdmin/_SiteAdminTopicsContent", viewModel)