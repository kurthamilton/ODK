@page "/siteadmin/groups"
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models.Components
@model GroupsModel
@inject IChapterSiteAdminService ChapterSiteAdminService
@{
    Layout = "_SiteAdminLayout";
    Model.Title = "Groups | SiteAdmin";

    var request = Model.MemberServiceRequest;
    var viewModel = await ChapterSiteAdminService.GetSiteAdminChaptersViewModel(request);
}

@await Html.PartialAsync("SiteAdmin/_SiteAdminGroupsContent", viewModel)