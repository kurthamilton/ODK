@page "/siteadmin/issues/{id:guid}"
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@using ODK.Services.Issues
@model IssueModel
@inject IIssueAdminService IssueAdminService
@{
    Layout = "_SiteAdminLayout";
    Model.Title = "Issues";

    var viewModel = await IssueAdminService.GetIssueAdminPageViewModel(Model.MemberServiceRequest, Model.IssueId);
}

@await Html.PartialAsync("SiteAdmin/_IssueContent", viewModel)