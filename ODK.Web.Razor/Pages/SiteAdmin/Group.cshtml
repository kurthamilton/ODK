@page "/siteadmin/groups/{id:guid}"
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models.Components
@model GroupModel
@inject IChapterSiteAdminService ChapterSiteAdminService
@{
    Layout = "_SiteAdminLayout";
    Model.Title = "Group | SiteAdmin";

    var request = Model.MemberServiceRequest;
    var viewModel = await ChapterSiteAdminService.GetSiteAdminChapterViewModel(request, Model.ChapterId);
}

@await Html.PartialAsync("SiteAdmin/_SiteAdminGroupContent", viewModel)