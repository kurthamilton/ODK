@page "/superadmin/groups"
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.SuperAdmin.GroupsModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_SiteAdminLayout";
    Model.Title = "Groups | SuperAdmin";

    var viewModel = await ChapterAdminService.GetSuperAdminChaptersViewModel(Model.MemberServiceRequest);
}

@await Html.PartialAsync("SuperAdmin/_ChaptersContent", viewModel)