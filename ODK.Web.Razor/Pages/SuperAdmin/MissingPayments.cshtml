@page "/superadmin/payments/missing"
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@using ODK.Services.Payments
@model ODK.Web.Razor.Pages.SuperAdmin.MissingPaymentsModel
@inject IPaymentAdminService PaymentAdminService
@{
    Layout = "_SiteAdminLayout";
    Model.Title = "Missing payments | SuperAdmin";

    var missingPayments = await PaymentAdminService.GetMissingPayments(Model.CurrentMemberId);
}

@await Html.PartialAsync("SuperAdmin/_MissingPaymentsContent", missingPayments)
