@page "/superadmin/payments"
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@using ODK.Services.Settings
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.SuperAdmin.PaymentsModel
@inject ISettingsService SettingsService
@{
    Layout = "_SiteAdminLayout";
    Model.Title = "Payments | SuperAdmin";

    var settings = await SettingsService.GetSitePaymentSettings(Model.CurrentMemberId);
}

@await Html.PartialAsync("SuperAdmin/_PaymentsContent", settings)
