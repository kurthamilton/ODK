@page "/my/groups/{id:guid}/events/venues/{venueId:guid}"
@attribute [Authorize]
@using Microsoft.AspNetCore.Authorization
@using ODK.Services.Venues
@model VenueModel
@inject IVenueAdminService VenueAdminService
@{
    Layout = "_GroupAdminLayout";

    var viewModel = await VenueAdminService.GetVenueViewModel(Model.AdminServiceRequest, Model.VenueId);
}

@await Html.PartialAsync("Admin/Venues/_VenueContent", viewModel)

@section scripts
{
    @await Html.PartialAsync("Components/_GoogleLocationScripts")
}