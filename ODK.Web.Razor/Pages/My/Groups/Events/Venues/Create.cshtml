@page "/my/groups/{id:guid}/events/venues/new"
@using ODK.Core.Chapters
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models
@model CreateModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_GroupAdminLayout";
    Model.Title = "Create venue";

    var chapter = await ChapterAdminService.GetChapter(Model.AdminServiceRequest);

    var viewModel = new OdkComponentViewModel<Chapter>(Model.ComponentContext, chapter);
}

@await Html.PartialAsync("Admin/Venues/_VenueCreateContent", viewModel)

@section scripts
{
    @await Html.PartialAsync("Components/Scripts/_GoogleLocation")
}