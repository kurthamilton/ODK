@page "/my/groups/{id:guid}/events/{eventId:guid}/attendees"
@using ODK.Services.Events
@model ODK.Web.Razor.Pages.My.Groups.Events.Event.AttendeesModel
@inject IEventAdminService EventAdminService
@{
    Layout = "_GroupAdminLayout";
    Model.Title = "Event attendees";

    var viewModel = await EventAdminService.GetEventAttendeesViewModel(Model.AdminServiceRequest, Model.EventId);
}

@await Html.PartialAsync("Admin/Events/_EventAttendeesContent", viewModel)