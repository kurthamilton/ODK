@page "/my/groups/{id:guid}/events/{eventId:guid}/tickets"
@using ODK.Services.Events
@model TicketsModel
@inject IEventAdminService EventAdminService
@{
    Layout = "_GroupAdminLayout";
    Model.Title = "Event tickets";

    var viewModel = await EventAdminService.GetEventTicketsViewModel(Model.AdminServiceRequest, Model.EventId);
}

@await Html.PartialAsync("Admin/Events/Event/_EventTicketsContent", viewModel)