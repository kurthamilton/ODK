@page "/my/groups/{id:guid}/members/subscriptions/new"
@using ODK.Services.Members
@model ODK.Web.Razor.Pages.My.Groups.Members.SubscriptionCreateModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_GroupAdminLayout";
    Model.Title = "Create subscription";

    var viewModel = await MemberAdminService.GetMemberSubscriptionCreateViewModel(Model.AdminServiceRequest);
}

@await Html.PartialAsync("Admin/Members/_SubscriptionCreateContent", viewModel)