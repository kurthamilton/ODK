@page "/my/groups/{id:guid}/members/{memberId:guid}"
@using ODK.Services.Members
@model ODK.Web.Razor.Pages.My.Groups.Members.MemberModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_GroupAdminLayout";

    var viewModel = await MemberAdminService.GetMemberViewModel(Model.AdminServiceRequest, Model.MemberId);
    
    Model.Title = viewModel.Member.FullName;
}

@await Html.PartialAsync("Admin/Members/_MemberContent", viewModel)
