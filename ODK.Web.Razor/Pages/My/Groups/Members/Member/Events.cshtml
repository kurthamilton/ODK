@page "/my/groups/{id:guid}/members/{memberId:guid}/events"
@using ODK.Services.Members
@model ODK.Web.Razor.Pages.My.Groups.Members.Member.EventsModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_GroupAdminLayout";

    var viewModel = await MemberAdminService.GetMemberEventsViewModel(Model.AdminServiceRequest, Model.MemberId);

    Model.Title = viewModel.Member.GetDisplayName(viewModel.Chapter.Id);
}

@await Html.PartialAsync("Admin/Members/_MemberEventsContent", viewModel)