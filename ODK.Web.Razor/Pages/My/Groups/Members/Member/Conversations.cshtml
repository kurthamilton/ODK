@page "/my/groups/{id:guid}/members/{memberId:guid}/conversations"
@using ODK.Services.Members
@model ConversationsModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_GroupAdminLayout";

    var viewModel = await MemberAdminService.GetMemberConversationsViewModel(Model.AdminServiceRequest, Model.MemberId);

    Model.Title = viewModel.Member.GetDisplayName(viewModel.Chapter.Id);
}

@await Html.PartialAsync("Admin/Members/_MemberConversationsContent", viewModel)