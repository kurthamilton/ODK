@page "/my/groups/{id:guid}/members/subscriptions/{subscriptionId:guid}"
@using ODK.Services.Members
@model ODK.Web.Razor.Pages.My.Groups.Members.SubscriptionEditModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_GroupAdminLayout";
    Model.Title = "Edit subscription";

    var viewModel = await MemberAdminService.GetMemberSubscriptionViewModel(Model.AdminServiceRequest, Model.SubscriptionId);
}

@await Html.PartialAsync("Admin/Members/_SubscriptionEditContent", viewModel)