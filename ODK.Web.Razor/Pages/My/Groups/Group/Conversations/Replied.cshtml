@page "/my/groups/{id:guid}/conversations/replied"
@using ODK.Services.Chapters
@model RepliedModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_GroupAdminLayout";
    Model.Title = "Conversations";

    var viewModel = await ChapterAdminService.GetChapterConversationsViewModel(Model.AdminServiceRequest, replied: true);
}

@await Html.PartialAsync("Admin/Chapter/Conversations/_ConversationsContent", viewModel)