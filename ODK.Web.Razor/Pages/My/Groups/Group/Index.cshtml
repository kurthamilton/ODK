@page "/my/groups/{id:guid}"
@using ODK.Services.Chapters
@model IndexModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_GroupAdminLayout";

    var viewModel = await ChapterAdminService.GetChapterAdminPageViewModel(Model.AdminServiceRequest);

    Model.Title = viewModel.Chapter.Name;
}

@await Html.PartialAsync("Groups/_GroupEditContent", viewModel)