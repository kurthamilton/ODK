@page "/my/groups/{id:guid}/delete"
@using ODK.Services.Chapters
@model DeleteModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_GroupAdminLayout";
    Model.Title = "Delete";

    var viewModel = await ChapterAdminService.GetChapterDeleteViewModel(Model.AdminServiceRequest);
}

@await Html.PartialAsync("Admin/Chapter/_DeleteContent", viewModel)