@page "/my/groups/{id:guid}/venues/{venueId:guid}/events"
@using ODK.Services.Venues
@model ODK.Web.Razor.Pages.My.Groups.Venues.Venue.EventsModel
@inject IVenueAdminService VenueAdminService
@{
    Layout = "_GroupAdminLayout";

    var viewModel = await VenueAdminService.GetVenueEventsViewModel(Model.AdminServiceRequest, Model.VenueId);

    Model.Title = viewModel.Venue.Name;
}

@await Html.PartialAsync("Admin/Venues/_VenueEventsContent", viewModel)
