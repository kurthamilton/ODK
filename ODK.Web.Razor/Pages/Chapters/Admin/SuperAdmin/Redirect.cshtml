@page "/{chapterName}/Admin/SuperAdmin/Redirect"
@using ODK.Web.Razor.Models.Admin
@model RedirectModel
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Redirect | SuperAdmin";
}

@await Html.PartialAsync("Admin/_AdminBody", new AdminBodyViewModel
{
    Title = "Redirect",
    Chapter = Model.Chapter,
    ContentFunc = @<div>@await Html.PartialAsync("Chapters/SuperAdmin/_RedirectContent", Model.Chapter)</div>
})