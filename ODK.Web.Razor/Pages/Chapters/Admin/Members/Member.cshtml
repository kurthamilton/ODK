@page "/{chapterName}/Admin/Members/{id:guid}"
@using ODK.Services.Members
@using ODK.Web.Razor.Models.Components
@using ODK.Web.Common.Components
@using ODK.Web.Razor.Models.Admin.Members
@model ODK.Web.Razor.Pages.Chapters.Admin.Members.MemberModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Edit member | Admin";

    var viewModel = await MemberAdminService.GetMemberViewModel(Model.AdminServiceRequest, Model.MemberId);
}

@await Html.PartialAsync("Admin/Members/_MemberContent", viewModel)