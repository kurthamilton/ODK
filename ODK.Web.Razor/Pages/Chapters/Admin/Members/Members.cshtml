@page "/{chapterName}/Admin/Members"
@using ODK.Services
@using ODK.Services.Members
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Members.MembersModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Members | Admin";

    var request = new AdminServiceRequest(Model.Chapter.Id, Model.CurrentMemberId);
    var viewModel = await MemberAdminService.GetMembersViewModel(request);
}

@await Html.PartialAsync("Admin/Members/_MembersContent", viewModel)

@section scripts
{
    <script src="~/js/odk.lists.js" asp-append-version="true"></script>
}