@page "/{chapterName}/Admin/Members/Subscriptions/{id:guid}"
@using ODK.Services
@using ODK.Services.Members
@using ODK.Web.Razor.Models.Components
@using ODK.Web.Common.Components
@using ODK.Web.Razor.Models.Admin.Members
@model ODK.Web.Razor.Pages.Chapters.Admin.Members.SubscriptionEditModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Edit subscription | Admin";

    var request = new AdminServiceRequest(Model.Chapter.Id, Model.CurrentMemberId);
    var viewModel = await MemberAdminService.GetMemberSubscriptionViewModel(request, Model.SubscriptionId);
}

@await Html.PartialAsync("Admin/Members/_SubscriptionEditContent", viewModel)