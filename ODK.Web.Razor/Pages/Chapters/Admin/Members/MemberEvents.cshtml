@page "/{chapterName}/Admin/Members/{id:guid}/Events"
@using ODK.Services.Members
@using ODK.Web.Razor.Models.Components
@using ODK.Web.Common.Components
@using ODK.Web.Razor.Models.Admin.Members
@model ODK.Web.Razor.Pages.Chapters.Admin.Members.MemberEventsModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "Admin/Members/_MemberAdminLayout";
    Model.Title = "Member events | Admin";

    var viewModel = await MemberAdminService.GetMemberEventsViewModel(Model.AdminServiceRequest, Model.MemberId);
}

@await Html.PartialAsync("Components/_Body", new BodyViewModel
{
    Content = await Html.PartialAsync("Admin/Members/_MemberEventsContent", viewModel)
})