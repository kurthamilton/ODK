@page "/{chapterName}/Admin/Members/{id:guid}/Email"
@using ODK.Services.Members
@using ODK.Web.Razor.Models.Admin.Members
@using ODK.Web.Razor.Models.Components
@using ODK.Web.Common.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Members.SendMemberEmailModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Send member email | Admin";

    var viewModel = await MemberAdminService.GetMemberEmailViewModel(Model.AdminServiceRequest, Model.MemberId);
}

@await Html.PartialAsync("Admin/Members/_SendMemberEmailContent", viewModel)