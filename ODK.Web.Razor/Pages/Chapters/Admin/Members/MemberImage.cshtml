@page "/{chapterName}/Admin/Members/{id:guid}/Image"
@using ODK.Services.Members
@using ODK.Web.Razor.Models.Admin.Members
@using ODK.Web.Razor.Models.Components
@using ODK.Web.Common.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Members.MemberImageModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Member image | Admin";

    var member = await MemberAdminService.GetMember(Model.AdminServiceRequest, Model.MemberId);
}

@await Html.PartialAsync("Admin/Members/_MemberImageContent", new MemberViewModel(Model.Chapter, member))

@section styles
{
    @await Html.PartialAsync("Components/_ImagingStyles")
}

@section scripts
{
    @await Html.PartialAsync("Components/_ImagingScripts")
}