@page "/{chapterName}/Admin/Members/Subscriptions"
@using ODK.Services
@using ODK.Services.Members
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Members.SubscriptionsModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "Admin/Members/_MemberAdminLayout";
    Model.Title = "Subscriptions | Admin";

    var request = new AdminServiceRequest(Model.Chapter.Id, Model.CurrentMemberId);
    var viewModel = await MemberAdminService.GetMemberSubscriptionsViewModel(request);
}

@await Html.PartialAsync("Components/_Body", new BodyViewModel
{
    Content = await Html.PartialAsync("Admin/Members/_SubscriptionsContent", viewModel)
})