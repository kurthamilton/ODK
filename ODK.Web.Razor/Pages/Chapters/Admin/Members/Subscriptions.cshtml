@page "/{chapterName}/Admin/Members/Subscriptions"
@using ODK.Services
@using ODK.Services.Members
@using ODK.Web.Razor.Models.Admin
@using ODK.Web.Razor.Models.Components
@model SubscriptionsModel
@inject IMemberAdminService MemberAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Subscriptions | Admin";

    var request = Model.MemberChapterServiceRequest;
    var viewModel = await MemberAdminService.GetMemberSubscriptionsViewModel(request);
}

@await Html.PartialAsync("Admin/_AdminBody", new AdminBodyViewModel
{
    Title = "Subscriptions",
    ContentFunc = @<div>@await Html.PartialAsync("Admin/Members/_SubscriptionsContent", viewModel)</div>
})