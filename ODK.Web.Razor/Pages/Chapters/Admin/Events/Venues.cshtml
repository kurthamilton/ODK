@page "/{chapterName}/Admin/Events/Venues"
@using ODK.Services
@using ODK.Services.Venues
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Events.VenuesModel
@inject IVenueAdminService VenueAdminService
@{
    Layout = "Admin/Events/_EventAdminLayout";
    Model.Title = "Venues | Admin";

    var request = new AdminServiceRequest(Model.Chapter.Id, Model.CurrentMemberId);
    var viewModel = await VenueAdminService.GetVenuesViewModel(request);
}

@await Html.PartialAsync("Components/_Body", new BodyViewModel
{
    Content = await Html.PartialAsync("Admin/Venues/_VenuesContent", viewModel)
})

@section scripts
{
    <script src="~/js/odk.lists.js" asp-append-version="true"></script>
}