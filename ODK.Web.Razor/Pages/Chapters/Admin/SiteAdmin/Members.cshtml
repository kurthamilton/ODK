@page "/{chapterName}/admin/siteadmin/members"
@using ODK.Core.Chapters
@using ODK.Web.Razor.Models
@using ODK.Web.Razor.Models.Admin
@using ODK.Web.Razor.Models.Components
@model MembersModel
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Members | SiteAdmin";

    var chapter = await Model.GetChapter();
    var viewModel = new OdkComponentViewModel<Chapter>(Model.ComponentContext, chapter);
}

@await Html.PartialAsync("Admin/_AdminBody", new AdminBodyViewModel
{
    ContentFunc = @<div>@await Html.PartialAsync("SiteAdmin/_MembersContent", viewModel)</div>
})