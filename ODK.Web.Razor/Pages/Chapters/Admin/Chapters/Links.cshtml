@page "/{chapterName}/Admin/Chapter/social-media"
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Chapters.LinksModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Links | Admin";

    var viewModel = await ChapterAdminService.GetChapterLinksViewModel(Model.AdminServiceRequest);
}

@await Html.PartialAsync("Admin/Chapter/_LinksContent", viewModel)