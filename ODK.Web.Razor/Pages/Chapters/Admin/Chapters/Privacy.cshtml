@page "/{chapterName}/Admin/Chapter/Privacy"
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Chapters.PrivacyModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Privacy | Admin";

    var viewModel = await ChapterAdminService.GetChapterPrivacyViewModel(Model.AdminServiceRequest);
}

@await Html.PartialAsync("Admin/Chapter/_PrivacyContent", viewModel)