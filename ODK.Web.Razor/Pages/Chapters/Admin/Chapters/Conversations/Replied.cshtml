@page "/{chapterName}/Admin/Chapter/Conversations/Read"
@using ODK.Services.Chapters
@model RepliedModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Conversations";

    var viewModel = await ChapterAdminService.GetChapterConversationsViewModel(Model.AdminServiceRequest, readByChapter: true);
}

@await Html.PartialAsync("Admin/Chapter/Conversations/_ConversationsContent", viewModel)