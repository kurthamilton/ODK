@page "/{chapterName}/Admin/Chapter/Messages"
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Chapters.MessagesModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "Admin/Chapters/_ChapterAdminLayout";
    Model.Title = "Messages | Admin";

    var viewModel = await ChapterAdminService.GetChapterMessagesViewModel(Model.AdminServiceRequest);
}

@await Html.PartialAsync("Components/_Body", new BodyViewModel
{
   Content = await Html.PartialAsync("Admin/Chapter/_MessagesContent", viewModel)
})