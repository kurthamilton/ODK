@page "/{chapterName}/Admin/Chapter/Questions/{id:guid}"
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models.Components
@using ODK.Web.Common.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Chapters.QuestionEditModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "Admin/Chapters/_ChapterAdminLayout";
    Model.Title = "Edit question | Admin";

    var viewModel = await ChapterAdminService.GetChapterQuestionViewModel(Model.AdminServiceRequest, Model.Question.Id);
}

@await Html.PartialAsync("Components/_Body", new BodyViewModel
{
    Content = await Html.PartialAsync("Admin/Chapter/_QuestionEditContent", viewModel)
})
