@page "/{chapterName}/Admin/Chapter/Questions"
@using ODK.Services.Chapters
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Chapters.QuestionsModel
@inject IChapterAdminService ChapterAdminService
@{
    Layout = "_OdkChapterAdminLayout";
    Model.Title = "Questions | Admin";

    var viewModel = await ChapterAdminService.GetChapterQuestionsViewModel(Model.AdminServiceRequest);
}

@await Html.PartialAsync("Admin/Chapter/_QuestionsContent", viewModel)