@page "/Pricing"
@using ODK.Services.Subscriptions
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.PricingModel
@inject ISiteSubscriptionService SiteSubscriptionService
@{
    Layout = "_HomeLayout";
    Model.Title = "Pricing";
    Model.Path = "/pricing";

    var dto = await SiteSubscriptionService.GetSiteSubscriptionsViewModel(Model.CurrentMemberIdOrDefault);
}

@await Html.PartialAsync("Components/_Body", new BodyViewModel
{
    Content = await Html.PartialAsync("Pricing/_PricingContent", dto),
    Title = "Pricing"
})