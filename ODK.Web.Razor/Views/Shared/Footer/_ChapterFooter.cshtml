@using ODK.Core.Platforms
@model ODK.Web.Razor.Models.Footer.FooterViewModel
@inject IPlatformProvider PlatformProvider
@{
    var platform = PlatformProvider.GetPlatform();
}

@if (platform == PlatformType.DrunkenKnitwits)
{
    @await Html.PartialAsync("Footer/_OdkFooter", Model)
}
else
{
    @await Html.PartialAsync("Footer/_SiteFooter", Model)
}