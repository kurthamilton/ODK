@model ChapterConversationsAdminPageViewModel
@using ODK.Services.Chapters.ViewModels
@using ODK.Web.Common.Components
@using ODK.Web.Common.Routes
@using ODK.Web.Razor.Models.Components
@{
    var menuItems = new[]
    {
        new MenuItem { Link = OdkRoutes.MemberGroups.GroupConversations(Model.Platform, Model.Chapter), Text = "Unread" },
        new MenuItem { Link = OdkRoutes.MemberGroups.GroupConversationsRead(Model.Platform, Model.Chapter), Text = "Read" }
    };
}

@await Html.PartialAsync("Components/_Tabs", new TabsViewModel
{
    MenuItems = menuItems
})