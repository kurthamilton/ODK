@using ODK.Web.Razor.Models.Members
@model ODK.Web.Razor.Models.Members.MemberContentViewModel

<div class="row">
    <div class="col-md-4 text-right order-md-2">
        @await Html.PartialAsync("Members/_MemberImage", new MemberImageViewModel(Model.Chapter, Model.Member))
    </div>
    <div class="col-md-8 order-md-1">
        @await Html.PartialAsync("Members/_MemberProfile", new MemberProfileViewModel(Model.Member, Model.CurrentMember))
    </div>    
</div>