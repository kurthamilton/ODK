@using ODK.Services.Settings
@inject ISettingsService SettingsService
@{
    var settings = await SettingsService.GetSiteSettings();
    var apiKey = settings.GoogleMapsApiKey;
}

<script src="https://maps.googleapis.com/maps/api/js?key=@apiKey&libraries=places&v=beta"></script>
<script src="~/js/odk.google.places.js" asp-append-version="true"></script>
