@model ODK.Web.Razor.Models.SuperAdmin.TimeZoneFormViewModel
@{
    var timeZoneOptions = TimeZoneInfo
        .GetSystemTimeZones()
        .Select(x => new SelectListItem { Value = x.Id, Text = x.DisplayName });
}

<div class="form-group mb-3">
    @Html.LabelFor(x => x.TimeZone, new { @class = "form-label" })
    @Html.DropDownListFor(x => x.TimeZone, timeZoneOptions, "", new { @class = "form-control" })
</div>