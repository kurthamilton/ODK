@using ODK.Web.Common.Components
@using ODK.Web.Common.Routes
@using ODK.Web.Razor.Models.Components
@using ODK.Web.Razor.Models.Groups.Profile
@model GroupProfileMenuViewModel
@{
    var menuItems = new MenuItem[]
    {
        new MenuItem { Link = OdkRoutes.Groups.Profile.Index(Model.Chapter), Text = "Profile" },      
        new MenuItem { Link = OdkRoutes.Groups.Profile.Subscription(Model.Chapter), Text = "My subscription" }
    };
}

@await Html.PartialAsync("Components/_SideMenu", new SideMenuViewModel
{
    MenuItems = menuItems
})