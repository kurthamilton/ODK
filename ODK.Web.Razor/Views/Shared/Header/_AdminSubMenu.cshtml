@using ODK.Web.Razor.Models.Header
@model IReadOnlyCollection<ODK.Web.Common.Components.MenuItem>
@{
    if (Model.Count == 0)
    {
        return;
    }
}

@await Html.PartialAsync("Header/_Navbar", new NavbarViewModel
{
    Breakpoint = "sm",
    CssClass = "border-bottom border-dark",
    Color = "light",
    Compact = true,
    Fluid = true,
    HideAccountMenu = true,
    Id = "navbar-admin-sub",
    MenuItems = new []
    {
        Model.ToArray()
    }
})