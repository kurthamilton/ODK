@page "/{chapterName}/Admin/Chapter/Text"
@using ODK.Web.Razor.Models.Components
@model ODK.Web.Razor.Pages.Chapters.Admin.Chapters.TextModel
@{
    Layout = "Admin/Chapters/_ChapterAdminLayout";
    Model.Title = "Text | Admin";
}

@await Html.PartialAsync("Components/_Body", new BodyViewModel
{
    Content = await Html.PartialAsync("Admin/Chapter/_TextContent", Model.Chapter)
})