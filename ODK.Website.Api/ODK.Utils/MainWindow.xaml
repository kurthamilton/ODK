<Window x:Class="ODK.Utils.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ODK.Utils"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtData" AcceptsReturn="True" HorizontalAlignment="Left" Margin="43,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="481" Height="285"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="43,345,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <TextBox x:Name="txtSavePath" HorizontalAlignment="Left" AcceptsReturn="True" Margin="43,17,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="481"/>

    </Grid>
</Window>
